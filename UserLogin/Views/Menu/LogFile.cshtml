@model IEnumerable<string>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>LogFile</title>
    <style>
        h1 {
            background-color: #006080;
            color: white;
            width: 100%;
            font-family: "Courier New";
            text-align: center;
            height: 60px;
        }

        body {
            background: #ccf3ff;
        }
    </style>
</head>
<body>
    <div>
        <h1>Log File</h1>
        <a href="@Url.Action("IndexRole", "Home")">BACK</a>
        <table>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
